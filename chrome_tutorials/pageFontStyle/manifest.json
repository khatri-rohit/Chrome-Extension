{
    "manifest_version": 2,
    "name": "PageFontStyle",
    "version": "1.0",
    "description": "Changes font style on a specific page",
    "icons": {
        "128": "icon-128.png",
        "48": "icon-48.png",
        "16": "icon-16.png"
    },
    "page_action": {
        "default_icon": "icon-16.png",
        "default_popup": "popup.html",
        "default_title": "PageFontStyle"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions":[
        "tabs"
    ]
}